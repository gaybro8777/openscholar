{
    "name": "openscholar/openscholar",
    "description": "OpenScholar installation profile.",
    "type": "drupal-profile",
    "license": "proprietary",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/purl",
                "version": "master",
                "type": "drupal-module",
                "dist": {
                    "url": "https://github.com/openscholar/purl-d8/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/openscholar/purl-d8.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/kenwheeler/slick.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mreq/slick-lightbox",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/mreq/slick-lightbox/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/mreq/slick-lightbox.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.7",
        "drupal/purl": "master@dev",
        "drupal/group_purl": "1.0-alpha1",
        "drupal/search_api": "^1.9",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "drupal/pathauto": "^1.2",
        "drupal/config_sync": "^2.0@alpha",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/paragraphs": "^1.4",
        "drupal/video_embed_field": "^2.0",
        "drupal/context": "^4.0@beta",
        "drupal/redirect": "^1.3",
        "drupal/toolbar_menu": "^2.1",
        "drupal/views_accordion": "^1.1",
        "drupal/slick": "^1.0",
        "kenwheeler/slick": "master@dev",
        "drupal/block_visibility_groups": "^1.3",
        "mreq/slick-lightbox": "master",
        "drupal/image_widget_crop": "^2.2",
        "zaporylie/composer-drupal-optimizations": "^1.0",
        "drupal/metatag": "^1.7",
        "drupal/admin_toolbar": "^1.25",
        "drupal/fullcalendar": "1.x-dev",
        "drupal/date_recur": "^2.0",
        "drupal/views_ical": "1.x-dev",
        "fullcalendar/fullcalendar": "^3.3",
        "drupal/date_recur_interactive": "^2.0",
        "drupal/bootstrap": "^3.17",
        "drupal/styleguide": "^1.0@alpha",
        "drupal/config_split": "^1.4",
        "twbs/bootstrap-sass": "^3.3.7",
        "drupal/mailchimp": "^1.8"
    },
    "require-dev": {
        "drupal/features": "^3.7",
        "webflo/drupal-core-require-dev": "^8.6.7",
        "drupal/devel": "^1.2",
        "weitzman/drupal-test-traits": "^1.0@beta",
        "dmore/chrome-mink-driver": "^2.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "drupal/core": "dist"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::placeProfile",
            "DrupalProject\\composer\\ScriptHandler::installFullcalendarLibrary",
            "DrupalProject\\composer\\ScriptHandler::installBootstrapLibrary",
            "./scripts/register-cs.sh",
            "./scripts/install-hooks.sh"
        ],
        "post-update-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::placeProfile",
            "DrupalProject\\composer\\ScriptHandler::installFullcalendarLibrary",
            "DrupalProject\\composer\\ScriptHandler::installBootstrapLibrary"
        ],
        "code-standard": [
            "./vendor/bin/phpcs --standard=Drupal",
            "./vendor/bin/phpcs --standard=DrupalPractice"
        ],
        "code-standard-fix": [
            "./vendor/bin/phpcbf --standard=Drupal"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/core": {
                "Make sure TestSitePath exist before creating .htkey": "https://www.drupal.org/files/issues/2246725-24.patch",
                "Views Date Filter Datetime Granularity Option": "https://www.drupal.org/files/issues/2018-12-11/views_date_filter_granularity_2868014-60.patch"
            },
            "drupal/group_purl": {
                "Prevent base path from interfering with purl detection": "https://www.drupal.org/files/issues/2019-01-17/group_purl.3026646-2.basepath.patch"
            },
            "drupal/fullcalendar": {
                "Fullcalendar incompatible with Date Recur 2.x": "https://www.drupal.org/files/issues/2019-01-09/3023612-5.patch"
            },
            "drupal/date_recur": {
                "Make compatible with Date All Day module (All day occurrences)": "https://www.drupal.org/files/issues/2019-01-17/2982275-29.patch"
            },
            "drupal/views_ical": {
                "Define missing views methods": "https://www.drupal.org/files/issues/2019-01-10/2982848-2.patch",
                "No page title due to undefined variable $config": "https://www.drupal.org/files/issues/2891778-2-undefined-variable-notice-fix.patch",
                "Add HTTP Header: application/calendar": "https://www.drupal.org/files/issues/views_ical-set_response_header-2601522-4.patch"
            },
            "drupal/block_visibility_groups": {
                "8.6: error when deleting a block visibility group": "https://www.drupal.org/files/issues/2018-10-18/block_visibility_groups_3000396-4.patch"
            },
            "drupal/bootstrap": {
                "Unsupported operand types during providers JSON discovery #8": "https://www.drupal.org/files/issues/2019-01-24/3027569-8.x-3.x-8.patch"
            },
            "drupal/mailchimp": {
                "Cache read problem in mailchimp_get_mergevars()": "https://www.drupal.org/files/issues/2019-02-13/3032674-cache-fix-for-mailchimp_get_mergevars-3.patch"
            }
        }
    }
}
